<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<!-- insert your AIML categories here -->
	<category>
        <pattern>UIUX DEVELOPER</pattern>
        <template>
            <think>
                <set name="job">UI/UX DEVELOPER</set>
                <set name="description">Tech savvy people with a creative side succeed in this role.</set>
                <set name="link"></set>
            </think>
            <srai>RECOMMEND JOB</srai>
        </template>
    </category>
    
    <category>
        <pattern>PROJECT MANAGER</pattern>
        <template>
            <think>
                <set name="job">PROJECT MANAGER</set>
                <set name="description">If you're a good communicator with outstanding leadership and organizational skills, this job's for you!</set>
                <set name="link"></set>
            </think>
            <srai>RECOMMEND JOB</srai>
        </template>
    </category>
    
    <category>
        <pattern>CYBERSECURITY ANALYST</pattern>
        <template>
            <think>
                <set name="job">CYBERSECURITY ANALYST</set>
                <set name="description">Resourceful, analytical problem solvers do well in this role!</set>
                <set name="link"></set>
            </think>
            <srai>RECOMMEND JOB</srai>
        </template>
    </category>
    
    <category>
        <pattern>DATA ENGINEER</pattern>
        <template>
            <think>
                <set name="job">DATA ENGINEER</set>
                <set name="description">Extracting, transforming, and loading massive amounts of data is what this role is all about!</set>
                <set name="link"></set>
            </think>
            <srai>RECOMMEND JOB</srai>
        </template>
    </category>
    
    <category>
        <pattern>BACKEND DEVELOPER</pattern>
        <template>
            <think>
                <set name="job">BACKEND DEVELOPER</set>
                <set name="description">A backend developer is responsible for server-side web application logic and integration of the work front-end developers do.</set>
                <set name="link"></set>
            </think>
            <srai>RECOMMEND JOB</srai>
        </template>
    </category>
    
    <category>
        <pattern>RECOMMEND JOB</pattern>
        <template>
            Let's go!<br/><br/>
            Now that I've gotten to know you, I think you'd love being a <get name="job"/>!<br/><br/>
            <get name="description"/><br/><br/>
            Learn more about this role here:
            <link>
                <text><get name="job"/></text>
                <url><get name="link"/></url>
            </link>
        </template>
    </category>
    
</aiml>